                    "data": {
                      "tipe": "object",
                      "properties": {
                        "updatedUser": {
                          "type": "object",
                          "description": "Data of the user",
                          "properties": {
                            "name": {
                              "type": "string",
                              "description": "Name of the user",
                              "example": "John Doe"
                            }
