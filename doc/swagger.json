    "/auth/update": {
      "put": {
        "tags": ["Users"],
        "security": [
          {
            "bearerToken": []
          }
        ],
        "summary": "Upadate a user",
        "description": "Upadate a user",
        "requestBody": {
          "description": "Request body for Upadate",
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "name": {
                    "type": "string",
                    "description": "Email of the user",
                    "example": "John Doe"
                  },
                  "phoneNumber": {
                    "type": "string",
                    "description": "Phone Number of the user",
                    "example": "1234567890"
                  },
                  "country": {
                    "type": "string",
                    "description": "Country of the user",
                    "example": "Indonesia"
                  },
                  "city": {
                    "type": "string",
                    "description": "City of the user",
                    "example": "Jakarta"
                  },
                  "image": {
                    "type": "string",
                    "format": "binary",
                    "description": "Image of the user"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "User Updated successfully",
